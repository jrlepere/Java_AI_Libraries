import java.util.Arrays;

import games.Action;

public class Mark implements Action<Board> {

	public Mark(Symbol S, int row, int col) {
		this.S = S;
		this.row = row;
		this.col = col;
	}
	
	public Board execute(Board s) {
		Symbol[][] board = Arrays.copyOf(s.getBoard(), Board.BOARD_SIZE);
		return null;
	}
	
	private Symbol S;
	private int row;
	private int col;

}
