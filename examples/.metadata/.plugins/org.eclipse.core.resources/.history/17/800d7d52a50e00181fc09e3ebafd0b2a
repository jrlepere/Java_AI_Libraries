import java.awt.Color;
import java.util.List;

import csp.Variable;
import csp_search.CSPDepthSearch;
import search.IResultObject;

public class Main {

	public static void main(String[] args) {
		CAProblem p = new CAProblem();
		CSPDepthSearch ds = new CSPDepthSearch<>();
		IResultObject res = ds.execute(p);
		for (Variable v : (List<Variable<CADomain, Color>>) res.getObject("ASSIGNMENT")) {
			System.out.println(v);
		}
	}
	
}
