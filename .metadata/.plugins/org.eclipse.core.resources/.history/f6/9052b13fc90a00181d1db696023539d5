package search;

import java.util.TreeMap;

import problem.State;

public class LocalSearchResultObject extends AResultObject {

	public LocalSearchResultObject(State goalState) {
		super(test(goalState));
	}
	
	private static TreeMap<String, Object> test(State goalState) {
		TreeMap<String,Object> map = new TreeMap<>();
		map.put("GOAL_STATE", goalState);
		return map;
	}
	
}
