package search_algorithm;

import problem.Action;
import problem.Problem;

public class HillClimb extends LocalSearch {

	public HillClimb(ObjectiveFunction function) {
		super(function);
	}
	
	public ResultObject execute(Problem p) {
		Node currentNode = new Node(p.getInitialState(), null, null);
		while (true) {
			Node bestSuccessor = null;
			for (Action action : p.getActions()) {
				if (!action.canExecute(currentNode.getState())) continue;
				Node successor = action.execute(currentNode.getState());
			}
		}
		return null;
	}

}
